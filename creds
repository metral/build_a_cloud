export OS_USERNAME="" # Public Cloud Username
export OS_PASSWORD="" # Public Cloud API KEY
export OS_TENANT_NAME=$OS_USERNAME # Public Cloud Tenant Name (same as Username)
export OS_AUTH_SYSTEM="rackspace"
export OS_AUTH_URL="https://identity.api.rackspacecloud.com/v2.0/"
export OS_REGION_NAME="ORD" # ORD - Chicago | DFW - Dallas Fort-Worth 
